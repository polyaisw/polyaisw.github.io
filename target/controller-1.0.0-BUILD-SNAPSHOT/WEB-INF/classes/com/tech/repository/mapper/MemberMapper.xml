<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tech.repository.mapper.MemberMapper">
	
	<insert id="memberJoin">
 		INSERT INTO user(`email`, `password`, `name`,`rank`,`blacklist`, `profileimg`, `tel`,`address`,`ticket`)
 		 values(#{email}, #{password}, #{name},#{rank},#{blacklist}, #{profileimg}, #{tel},#{address},#{ticket})
	</insert>
	
</mapper>